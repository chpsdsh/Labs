#include <stdio.h>
#include <malloc.h>

int main() {
    int n;
    float * a;
    float ** b;
    scanf("%d", &n);
    a = (float *)malloc(n*sizeof(float));
    b = (float **)malloc(n*sizeof(float *));
    for(int i=0; i < n; i++) {
        scanf("%f", a + i);
        *(b + i) = a + i;
    }
    for(int j=0; j < n-1; j++)
        for(int i=0; i < n-1; i++)
            if (**(b+i) > **(b+i+1)) {
                float * tmp = *(b + i);
                *(b+i) = *(b + i + 1);
                *(b + i + 1) = tmp;
            }

    printf("\n");
    for(int i=0; i < n; i++)
        printf("%8.2f", *(a+i));
    printf("\n");
    for(int i=0; i < n; i++)
        printf("%8.2f", **(b+i));

    free(a);
    return 0;
}
