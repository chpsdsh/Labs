#include <stdio.h>
#include <malloc.h>
#define PRAKTIKA int main
PRAKTIKA() {
    int *a;
    int n;
    printf("vvedite kol-vo elementov \n");
    scanf_s("%d",&n);
    a=(int*) malloc(n*sizeof(int));
    for(int i=0;i<n;i++)
        scanf("%d",&a[i]);
    for(int i=0;i<n;i++)
        printf("%d\n", a[i]);
    }
